{
    "name": "citadel-example-app",
    "container": {
        "port": 80,
        "buildArgs": {}
    },
    "stages": {
        "dev": {
            "aws": {
                "account_name": "dev",
                "account": "932213710980",
                "region": "ap-southeast-1"
            },
            "vpc": {
                "name": "VPC"
            },
            "loadBalancer": {
                "healthcheckPath": "/"
            },
            "route53": {
                "enable": true,
                "domain": "demo1.dnx.host",
                "hostname": "myapp"
            },
            "acm": {
                "create": true
            },
            "secretsManager": [],
            "task": {
                "desiredCount": 1,
                "cpu": 256,
                "memoryLimitMiB": 512,
                "spot": true
            },
            "autoscaling": {
                "cpuTargetUtilizationPercent": 70,
                "minCapacity": 1,
                "maxCapacity": 4
            }
        }
    }
}